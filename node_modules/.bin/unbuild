#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/dist/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/dist/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules:/mnt/e/FrontendEngine/auto-scirpt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/unbuild@3.5.0_typescript@5.8.2/node_modules/unbuild/dist/cli.mjs" "$@"
fi
